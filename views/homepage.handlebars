<!-- Loop over each object in the daycares array -->
{{#each daycares as |daycare| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <a href="/bookdaycare">{{daycare.daycare_name}}</a>
    </h2>
    </div>
  <div class="col-md-7">
    <p>
      {{daycare.daycare_description}}
    </p>
  </div>
</div>
{{/each}}
